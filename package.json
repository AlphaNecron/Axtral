{
  "name": "void",
  "version": "1.0.0-prealpha",
  "private": true,
  "engines": {
    "node": ">=16"
  },
  "os": [
    "linux",
    "darwin"
  ],
  "scripts": {
    "dev": "NODE_ENV=development yarn start",
    "build": "yarn run build:next && yarn run build:schema",
    "build:next": "next build",
    "build:schema": "prisma generate --schema=prisma/schema.prisma",
    "start": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\" server.ts",
    "prepare": "husky install",
    "lint": "next lint --fix",
    "prod": "yarn build:next && yarn start"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} --transpile-only prisma/seed.ts"
  },
  "dependencies": {
    "@isaacs/ttlcache": "^1.1.0",
    "@mantine/core": "^4.2.12",
    "@mantine/dropzone": "^4.2.12",
    "@mantine/form": "^4.2.12",
    "@mantine/hooks": "^4.2.12",
    "@mantine/modals": "^4.2.12",
    "@mantine/next": "^4.2.12",
    "@mantine/notifications": "^4.2.12",
    "@mantine/prism": "^4.2.12",
    "@prisma/client": "^4.0.0",
    "@types/multer": "^1.4.7",
    "argon2": "^0.28.7",
    "discord-oauth2": "^2.10.0",
    "iron-session": "^6.1.3",
    "luxon": "^2.4.0",
    "multer": "^1.4.5-lts.1",
    "next": "12.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.4.0",
    "react-qrcode-logo": "^2.7.0",
    "swr": "^1.3.0",
    "systeminformation": "^5.11.22",
    "winston": "^3.8.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/luxon": "^2.3.2",
    "@types/node": "16.11.43",
    "@types/react": "^17.0.47",
    "@types/yup": "^0.29.14",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "commitizen": "^4.2.4",
    "commitizen-emoji": "^1.0.5",
    "eslint": "8.19.0",
    "eslint-config-next": "12.2.0",
    "husky": "^8.0.1",
    "prisma": "^4.0.0",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && yarn cz --hook || true"
    }
  },
  "packageManager": "yarn@3.2.0",
  "config": {
    "commitizenEmoji": {
      "conventionalFormat": true
    },
    "commitizen": {
      "path": "./node_modules/commitizen-emoji"
    }
  }
}
