{
  "name": "void",
  "version": "1.0.0-prealpha",
  "neutronVersion": "0.1.0-alpha",
  "private": true,
  "engines": {
    "node": "^16"
  },
  "os": [
    "linux",
    "darwin"
  ],
  "scripts": {
    "dev": "yarn start:dev",
    "build": "yarn run build:next",
    "build:next": "next build",
    "build:schema": "prisma generate",
    "analyze": "ANALYZE=true yarn build:next",
    "start:dev": "NODE_ENV=development yarn start",
    "start": "tsx src/void",
    "prepare": "husky install",
    "lint": "next lint --fix",
    "prod": "yarn build:next && yarn build:schema && yarn start"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@discordjs/rest": "^1.1.0",
    "@isaacs/ttlcache": "^1.2.0",
    "@mantine/core": "^5.3.0",
    "@mantine/dropzone": "^5.3.0",
    "@mantine/form": "^5.3.0",
    "@mantine/hooks": "^5.3.0",
    "@mantine/modals": "^5.3.0",
    "@mantine/next": "^5.3.0",
    "@mantine/notifications": "^5.3.0",
    "@mantine/prism": "^5.3.0",
    "@prisma/client": "^4.3.1",
    "argon2": "^0.29.1",
    "discord-api-types": "^0.37.8",
    "discord-oauth2": "^2.10.0",
    "discord.js": "^14.3.0",
    "fecha": "^4.2.3",
    "iron-session": "^6.2.1",
    "multer": "^1.4.5-lts.1",
    "next": "12.3.0",
    "picocolors": "^1.0.0",
    "pretty-ms": "^8.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.4.0",
    "react-qrcode-logo": "^2.8.0",
    "sharp": "^0.31.0",
    "swr": "^1.3.0",
    "systeminformation": "^5.12.6",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.3.0",
    "@types/multer": "^1.4.7",
    "@types/node": "16.11.58",
    "@types/react": "^17.0.49",
    "@types/sharp": "^0.30.5",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "commitizen": "^4.2.5",
    "commitizen-emoji": "^1.0.5",
    "eslint": "8.22.0",
    "eslint-config-next": "12.3.0",
    "husky": "^8.0.1",
    "prisma": "^4.3.1",
    "tsx": "^3.9.0",
    "typescript": "^4.8.3"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && yarn cz --hook || true"
    }
  },
  "packageManager": "yarn@3.2.3",
  "config": {
    "commitizenEmoji": {
      "conventionalFormat": true
    },
    "commitizen": {
      "path": "./node_modules/commitizen-emoji"
    }
  },
  "optionalDependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/server": "^11.10.0",
    "@prisma/generator-helper": "^4.3.1",
    "@prisma/internals": "^4.3.1",
    "@prisma/migrate": "^4.3.1"
  }
}
