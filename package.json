{
  "name": "void",
  "version": "1.0.0-prealpha",
  "private": true,
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "dev": "NODE_ENV=development ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\" server.ts",
    "build": "yarn run build:next && yarn run build:schema",
    "build:next": "next build",
    "build:schema": "prisma generate --schema=prisma/schema.prisma",
    "start": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\" server.ts",
    "prepare": "husky install",
    "lint": "next lint --fix"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"commonjs\"} prisma/seed.ts"
  },
  "dependencies": {
    "@mantine/core": "^4.1.2",
    "@mantine/dropzone": "^4.1.2",
    "@mantine/form": "^4.1.2",
    "@mantine/hooks": "^4.1.2",
    "@mantine/next": "^4.1.2",
    "@mantine/notifications": "^4.1.2",
    "@mantine/prism": "^4.1.2",
    "@next-auth/prisma-adapter": "^1.0.3",
    "@prisma/client": "^3.12.0",
    "argon2": "^0.28.5",
    "multer": "^1.4.4",
    "next": "12.1.4",
    "next-auth": "^4.3.1",
    "prisma": "^3.12.0",
    "qrcode.react": "^3.0.1",
    "react": "17",
    "react-dom": "17",
    "react-icons": "^4.3.1",
    "swr": "^1.2.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.43",
    "@types/yup": "^0",
    "@typescript-eslint/parser": "^5.18.0",
    "eslint": "8.12.0",
    "eslint-config-next": "12.1.4",
    "husky": "^7.0.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "parserPreset": "conventional-changelog-conventionalcommits",
    "rules": {
      "body-leading-blank": [
        1,
        "always"
      ],
      "body-max-line-length": [
        2,
        "always",
        100
      ],
      "footer-leading-blank": [
        1,
        "always"
      ],
      "footer-max-line-length": [
        2,
        "always",
        100
      ],
      "header-max-length": [
        2,
        "always",
        100
      ],
      "subject-case": [
        2,
        "never",
        [
          "sentence-case",
          "start-case",
          "pascal-case",
          "upper-case"
        ]
      ],
      "subject-empty": [
        2,
        "never"
      ],
      "subject-full-stop": [
        2,
        "never",
        "."
      ],
      "type-case": [
        2,
        "always",
        "lower-case"
      ],
      "type-empty": [
        2,
        "never"
      ],
      "type-enum": [
        2,
        "always",
        [
          "build",
          "chore",
          "ci",
          "feat",
          "fix",
          "perf",
          "refactor",
          "revert",
          "style",
          "test",
          "bump"
        ]
      ],
      "scope-enum": [
        1,
        "always",
        [
          "prisma",
          "scripts",
          "server",
          "docker",
          "readme",
          "pages",
          "config",
          "api",
          "hooks",
          "neutron",
          "components",
          "middleware",
          "redux",
          "lib",
          "assets"
        ]
      ]
    }
  },
  "packageManager": "yarn@3.2.0"
}
