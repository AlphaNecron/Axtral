{
  "name": "void",
  "version": "1.0.0-prealpha",
  "private": true,
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "dev": "NODE_ENV=development yarn start",
    "build": "yarn run build:next && yarn run build:schema",
    "build:next": "next build",
    "build:schema": "prisma generate --schema=prisma/schema.prisma",
    "start": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\" server.ts",
    "prod": "yarn run build && yarn run start",
    "prepare": "husky install",
    "lint": "next lint --fix"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} --transpile-only prisma/seed.ts"
  },
  "dependencies": {
    "@mantine/core": "^4.2.9",
    "@mantine/dropzone": "^4.2.9",
    "@mantine/form": "^4.2.9",
    "@mantine/hooks": "^4.2.9",
    "@mantine/modals": "^4.2.9",
    "@mantine/next": "^4.2.9",
    "@mantine/notifications": "^4.2.9",
    "@mantine/prism": "^4.2.9",
    "@next-auth/prisma-adapter": "^1.0.3",
    "@prisma/client": "^3.15.2",
    "argon2": "^0.28.5",
    "axios": "^0.27.2",
    "luxon": "^2.4.0",
    "multer": "^1.4.5-lts.1",
    "next": "12.1.6",
    "next-auth": "^4.5.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.4.0",
    "react-qrcode-logo": "^2.7.0",
    "swr": "^1.3.0",
    "systeminformation": "^5.11.20",
    "winston": "^3.7.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/luxon": "^2.3.2",
    "@types/node": "^17.0.45",
    "@types/react": "^17.0.46",
    "@types/yup": "^0.29.14",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "commitizen": "^4.2.4",
    "commitizen-emoji": "^1.0.5",
    "eslint": "8.17.0",
    "eslint-config-next": "12.1.6",
    "husky": "^8.0.1",
    "prisma": "^3.15.2",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && yarn cz --hook || true"
    }
  },
  "packageManager": "yarn@3.2.0",
  "config": {
    "commitizenEmoji": {
      "conventionalFormat": true
    },
    "commitizen": {
      "path": "./node_modules/commitizen-emoji"
    }
  }
}
